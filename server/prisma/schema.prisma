datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  id      Int    @id @default(autoincrement())
  name    String
  title   String
  about   String
}

model Skill {
  id      Int    @id @default(autoincrement())
  name    String
  level   String
}

model Project {
  id      Int    @id @default(autoincrement())
  title   String
  desc    String
  link    String 
}
